.center-form
  .center-form-inside
    %h1
      =t('.list')

    %table
      %thead
        %tr
          %th
            =t('.name')
          %th
            =t('.email')
          %th
      %tbody
        - @users.each do |user|
          %tr
            %td= user.name
            %td= user.email
            %td= link_to t('common.show'), user

    %br

    = link_to t('.new'), new_user_path
